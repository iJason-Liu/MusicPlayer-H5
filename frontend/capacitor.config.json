{
  "appId": "vip.crayon.diary",
  "appName": "music-player",
  "webDir": "h5",
  "server": {
    "androidScheme": "https",
    "cleartext": true,
    "allowNavigation": [
      "diary.crayon.vip"
    ]
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "launchAutoHide": true,
